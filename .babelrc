{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 versions",
            "Firefox ESR",
            "> 1%",
            "ie >= 8",
            "iOS >= 8",
            "Android >= 4"
          ]
        }
      }
    ]
  ],
  "env": {
    test: {
      plugins: ["transform-es2015-modules-commonjs"]
    }
  }
}